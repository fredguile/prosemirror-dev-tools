{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "react"
  ],
  "plugins": [
    "syntax-class-properties",
    "transform-class-properties",
    "transform-commonjs-es2015-modules"
  ],
  "env": {
    "production": {
      "presets": ["env", "react"],
      "plugins": [
        "syntax-class-properties",
        "transform-class-properties",
        "transform-commonjs-es2015-modules"
      ]
    }
  }
}
